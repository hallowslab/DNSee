{
    "manifest_version": 3,
    "name": "DNSee",
    "version": "1.0",
    "description": "Shows the current page's IP address and domain name.",
    "permissions": ["tabs"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    }
}
